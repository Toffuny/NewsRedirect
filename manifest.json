{
  "name": "News Reader",
  "version": "0.2",
  "description": "Checks URL and redirects as required.",
  "background": {
     "page":"bg.html"
     },
   "manifest_version": 2,
   "browser_action": {
     "default_icon": "newspaper.png"
   },
   "content_scripts": [
   {
     "matches": ["http://*/*", "https://*/*"],
     "js": ["content.js"]
   }
   ],
  "permissions": ["tabs"]
}
