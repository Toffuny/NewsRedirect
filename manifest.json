{
  "name": "Redirector",
  "description": "Redirect urls",
  "version": "1.0",
  "manifest_version": 2,
  "background": {"scripts":["background.js"]},
  "options_page": "options.html",
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "content_security_policy": "script-src 'self' https://*.googleapis.com https://*.bootstrapcdn.com; object-src 'self'"
}
